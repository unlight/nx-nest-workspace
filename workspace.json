{
  "version": 2,
  "cli": {
    "defaultCollection": "@nrwl/nest"
  },
  "defaultProject": "api",
  "generators": {
    "@nrwl/react": {
      "application": {
        "style": "css",
        "linter": "eslint",
        "babel": true
      },
      "component": {
        "style": "css"
      },
      "library": {
        "style": "css",
        "linter": "eslint"
      }
    },
    "@nrwl/web:application": {
      "style": "css",
      "linter": "eslint",
      "unitTestRunner": "jest",
      "e2eTestRunner": "none"
    },
    "@nrwl/web:library": {
      "style": "css",
      "linter": "eslint",
      "unitTestRunner": "jest"
    }
  },
  "projects": {
    "api": {
      "root": "apps/api",
      "sourceRoot": "apps/api/src",
      "projectType": "application",
      "targets": {
        "build": {
          "executor": "@nrwl/node:build",
          "outputs": [
            "{options.outputPath}"
          ],
          "options": {
            "outputPath": "dist/apps/api",
            "main": "apps/api/src/main.ts",
            "tsConfig": "apps/api/tsconfig.app.json",
            "assets": [
              "apps/api/src/assets"
            ],
            "webpackConfig": "webpack.config.js"
          },
          "configurations": {
            "production": {
              "optimization": true,
              "extractLicenses": false,
              "inspect": false,
              "fileReplacements": [
                {
                  "replace": "apps/api/src/environments/environment.ts",
                  "with": "apps/api/src/environments/environment.prod.ts"
                }
              ]
            }
          }
        },
        "serve": {
          "executor": "@nrwl/node:execute",
          "options": {
            "buildTarget": "api:build",
            "inspect": true,
            "port": 9277
          }
        },
        "lint": {
          "executor": "@nrwl/linter:eslint",
          "options": {
            "lintFilePatterns": [
              "apps/api/**/*.ts"
            ]
          }
        },
        "test": {
          "executor": "@nrwl/jest:jest",
          "outputs": [
            "coverage/apps/api"
          ],
          "options": {
            "jestConfig": "apps/api/jest.config.js",
            "passWithNoTests": true
          }
        },
        "e2e": {
          "executor": "@nrwl/jest:jest",
          "outputs": [
            "coverage/apps/api-e2e"
          ],
          "options": {
            "jestConfig": "apps/api/jest-e2e.config.js"
          }
        }
      }
    },
    "conduit.api": {
      "root": "apps/conduit.api",
      "sourceRoot": "apps/conduit.api/src",
      "projectType": "application",
      "targets": {
        "build": {
          "executor": "@nrwl/node:build",
          "outputs": [
            "{options.outputPath}"
          ],
          "options": {
            "outputPath": "dist/apps/conduit.api",
            "main": "apps/conduit.api/src/main.ts",
            "tsConfig": "apps/conduit.api/tsconfig.app.json"
          },
          "configurations": {
            "production": {
              "optimization": true,
              "extractLicenses": true,
              "inspect": false,
              "fileReplacements": [
                {
                  "replace": "apps/conduit.api/src/environments/environment.ts",
                  "with": "apps/conduit.api/src/environments/environment.prod.ts"
                }
              ]
            }
          }
        },
        "serve": {
          "executor": "@nrwl/node:execute",
          "options": {
            "buildTarget": "conduit.api:build"
          }
        },
        "lint": {
          "executor": "@nrwl/linter:eslint",
          "options": {
            "lintFilePatterns": [
              "apps/conduit.api/**/*.ts"
            ]
          }
        },
        "test": {
          "executor": "@nrwl/jest:jest",
          "outputs": [
            "coverage/apps/conduit.api"
          ],
          "options": {
            "jestConfig": "apps/conduit.api/jest.config.js",
            "passWithNoTests": true,
            "runInBand": true
          }
        },
        "tsc": {
          "executor": "@nrwl/workspace:run-commands",
          "options": {
            "cwd": "apps/conduit.api",
            "parallel": false,
            "commands": [
              "npx tsc -p tsconfig.app.json --noEmit --watch"
            ]
          }
        }
      }
    },
    "conduit.domain": {
      "root": "libs/conduit.domain",
      "sourceRoot": "libs/conduit.domain/src",
      "projectType": "library",
      "targets": {
        "lint": {
          "executor": "@nrwl/linter:eslint",
          "options": {
            "lintFilePatterns": [
              "libs/conduit.domain/**/*.ts"
            ]
          }
        },
        "test": {
          "executor": "@nrwl/jest:jest",
          "outputs": [
            "coverage/libs/conduit.domain"
          ],
          "options": {
            "jestConfig": "libs/conduit.domain/jest.config.js",
            "passWithNoTests": true
          }
        }
      }
    },
    "eventstore-publisher": {
      "root": "libs/eventstore-publisher",
      "sourceRoot": "libs/eventstore-publisher/src",
      "projectType": "library",
      "targets": {
        "lint": {
          "executor": "@nrwl/linter:eslint",
          "options": {
            "lintFilePatterns": [
              "libs/eventstore-publisher/**/*.ts"
            ]
          }
        },
        "test": {
          "executor": "@nrwl/jest:jest",
          "outputs": [
            "coverage/libs/eventstore-publisher"
          ],
          "options": {
            "jestConfig": "libs/eventstore-publisher/jest.config.js",
            "passWithNoTests": true
          }
        }
      }
    },
    "frontend": {
      "root": "apps/frontend",
      "sourceRoot": "apps/frontend/src",
      "projectType": "application",
      "targets": {
        "build": {
          "executor": "@nrwl/web:build",
          "outputs": [
            "{options.outputPath}"
          ],
          "options": {
            "outputPath": "dist/apps/frontend",
            "index": "apps/frontend/src/index.html",
            "main": "apps/frontend/src/main.tsx",
            "polyfills": "apps/frontend/src/polyfills.ts",
            "tsConfig": "apps/frontend/tsconfig.app.json",
            "assets": [
              "apps/frontend/src/favicon.ico",
              "apps/frontend/src/assets"
            ],
            "styles": [
              "apps/frontend/src/styles.css"
            ],
            "scripts": [],
            "webpackConfig": "apps/frontend/webpack.config.js"
          },
          "configurations": {
            "production": {
              "fileReplacements": [
                {
                  "replace": "apps/frontend/src/environments/environment.ts",
                  "with": "apps/frontend/src/environments/environment.prod.ts"
                }
              ],
              "optimization": true,
              "outputHashing": "all",
              "sourceMap": false,
              "extractCss": true,
              "namedChunks": false,
              "extractLicenses": false,
              "vendorChunk": false,
              "budgets": [
                {
                  "type": "initial",
                  "maximumWarning": "2mb",
                  "maximumError": "5mb"
                }
              ]
            }
          }
        },
        "serve": {
          "executor": "@nrwl/web:dev-server",
          "options": {
            "buildTarget": "frontend:build"
          },
          "configurations": {
            "production": {
              "buildTarget": "frontend:build:production"
            }
          }
        },
        "lint": {
          "executor": "@nrwl/linter:eslint",
          "options": {
            "lintFilePatterns": [
              "apps/frontend/**/*.{ts,tsx,js,jsx}"
            ]
          }
        },
        "test": {
          "executor": "@nrwl/jest:jest",
          "outputs": [
            "coverage/apps/frontend"
          ],
          "options": {
            "jestConfig": "apps/frontend/jest.config.js",
            "passWithNoTests": true
          }
        }
      }
    },
    "frontend-e2e": {
      "root": "apps/frontend-e2e",
      "sourceRoot": "apps/frontend-e2e/src",
      "projectType": "application",
      "targets": {
        "e2e": {
          "executor": "@nrwl/cypress:cypress",
          "options": {
            "cypressConfig": "apps/frontend-e2e/cypress.json",
            "tsConfig": "apps/frontend-e2e/tsconfig.e2e.json",
            "devServerTarget": "frontend:serve"
          },
          "configurations": {
            "production": {
              "devServerTarget": "frontend:serve:production"
            }
          }
        },
        "lint": {
          "executor": "@nrwl/linter:eslint",
          "options": {
            "lintFilePatterns": [
              "apps/frontend-e2e/**/*.{js,ts}"
            ]
          }
        }
      }
    },
    "vite": {
      "projectType": "application",
      "root": "apps/vite",
      "sourceRoot": "apps/vite/src",
      "targets": {
        "build": {
          "executor": "@nrwl/workspace:run-commands",
          "outputs": [
            "{options.outputPath}"
          ],
          "options": {
            "cwd": "apps/vite",
            "parallel": false,
            "commands": [
              "npx vite build"
            ]
          },
          "configurations": {
            "production": {}
          }
        },
        "serve": {
          "executor": "@nrwl/workspace:run-commands",
          "options": {
            "cwd": "apps/vite",
            "parallel": false,
            "commands": [
              {
                "command": "npx vite serve -d -l=info"
              }
            ]
          },
          "configurations": {
            "production": {
              "commands": [
                "npx tsc",
                "npx vite build",
                "npx vite preview"
              ]
            }
          }
        },
        "lint": {
          "executor": "@nrwl/linter:eslint",
          "options": {
            "lintFilePatterns": [
              "apps/vite/**/*.ts"
            ]
          }
        },
        "test": {
          "executor": "@nrwl/jest:jest",
          "outputs": [
            "coverage/apps/vite"
          ],
          "options": {
            "jestConfig": "apps/vite/jest.config.js",
            "passWithNoTests": true
          }
        }
      }
    }
  }
}